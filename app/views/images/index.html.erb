<h1>Images#index</h1>
<p>Find me in app/views/images/index.html.erb</p>

<h1> Welcome to Photogur </h1>
<%= link_to "New", images_new_url %>

<ul>
  <% @images.each do |image| %>
    <li><%= image[:title].upcase %></li>
    <li><%= image[:author] %></li>
    <li><%= link_to image_tag(image.url), "images/#{image.id}"%>
    </li>
  <% end %> 
</ul>